local UserInputService = game:GetService("UserInputService")

-- Fly
local flyingBV, flyConn
local function startFly()
    if state.flying then return end
    state.flying = true
    local char = getChar()
    local hrp = char:WaitForChild("HumanoidRootPart")
    flyingBV = Instance.new("BodyVelocity")
    flyingBV.MaxForce = Vector3.new(1e9,1e9,1e9)
    flyingBV.Parent = hrp
    flyConn = RunService.RenderStepped:Connect(function()
        local dir = Vector3.zero
        local cam = workspace.CurrentCamera
        if UserInputService:IsKeyDown(Enum.KeyCode.W) then dir += cam.CFrame.LookVector end
        if UserInputService:IsKeyDown(Enum.KeyCode.S) then dir -= cam.CFrame.LookVector end
        if UserInputService:IsKeyDown(Enum.KeyCode.A) then dir -= cam.CFrame.RightVector end
        if UserInputService:IsKeyDown(Enum.KeyCode.D) then dir += cam.CFrame.RightVector end
        if UserInputService:IsKeyDown(Enum.KeyCode.Space) then dir += Vector3.new(0,1,0) end
        if UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) then dir -= Vector3.new(0,1,0) end
        flyingBV.Velocity = (dir.Magnitude>0 and dir.Unit*state.flySpeed or Vector3.zero)
    end)
    notifyOk("Fly enabled")
end

local function stopFly()
    state.flying=false
    if flyConn then flyConn:Disconnect(); flyConn=nil end
    if flyingBV then flyingBV:Destroy(); flyingBV=nil end
    notifyNo("Fly disabled")
end

-- Fly tab
Tabs.Fly:CreateToggle({Name="Enable Fly", CurrentValue=false, Callback=function(v) if v then startFly() else stopFly() end end})
Tabs.Fly:CreateSlider({Name="Fly Speed", Range={20,400}, Increment=5, CurrentValue=state.flySpeed, Suffix=" speed", Callback=function(v) state.flySpeed=v end})
Tabs.Fly:CreateDropdown({Name="Fly Mode", Options={"Smooth","Vanilla","Rocket"}, CurrentOption=state.flyMode, Callback=function(v) state.flyMode=v end})

-- Infinite Jump
Tabs.Movement:CreateToggle({Name="Infinite Jump", CurrentValue=state.infJump, Callback=function(v) state.infJump=v notifyOk(v and "Infinite Jump enabled" or "Infinite Jump disabled") end})
game:GetService("UserInputService").JumpRequest:Connect(function()
    if state.infJump then
        local char = getChar()
        local hum = char:FindFirstChildOfClass("Humanoid")
        if hum then hum:ChangeState(Enum.HumanoidStateType.Jumping) end
    end
end)

-- Noclip
local noclipConn
Tabs.Movement:CreateToggle({Name="Noclip", CurrentValue=state.noclip, Callback=function(v)
    state.noclip=v
    if v then
        noclipConn=RunService.Stepped:Connect(function()
            local char=getChar()
            for _,p in pairs(char:GetDescendants()) do if p:IsA("BasePart") then p.CanCollide=false end end
        end)
        notifyOk("Noclip enabled")
    else
        if noclipConn then noclipConn:Disconnect(); noclipConn=nil end
        notifyNo("Noclip disabled")
    end
end})

-- WalkSpeed & JumpPower sliders
Tabs.Player:CreateSlider({Name="WalkSpeed", Range={16,400}, Increment=1, CurrentValue=state.walkspeed, Callback=function(v)
    state.walkspeed=v
    local hum=getChar():FindFirstChildOfClass("Humanoid")
    if hum then hum.WalkSpeed=v end
end})
Tabs.Player:CreateSlider({Name="JumpPower", Range={50,400}, Increment=1, CurrentValue=state.jumppower, Callback=function(v)
    state.jumppower=v
    local hum=getChar():FindFirstChildOfClass("Humanoid")
    if hum then hum.JumpPower=v end
end})

-- Loop after death
Tabs.Movement:CreateToggle({Name="Loop after death", CurrentValue=state.loopAfterDeath, Callback=function(v) state.loopAfterDeath=v notifyOk("Loop after death "..(v and "enabled" or "disabled")) end})
LocalPlayer.CharacterAdded:Connect(function()
    if state.loopAfterDeath then
        task.wait(0.5)
        if state.flying then startFly() end
        if state.noclip and not noclipConn then
            noclipConn=RunService.Stepped:Connect(function()
                local char=getChar()
                for _,p in pairs(char:GetDescendants()) do if p:IsA("BasePart") then p.CanCollide=false end end
            end)
        end
        local hum = getChar():FindFirstChildOfClass("Humanoid")
        if hum then hum.WalkSpeed=state.walkspeed; hum.JumpPower=state.jumppower end
        notifyOk("Utilities reapplied after respawn")
    end
end)

