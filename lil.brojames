-- ESP Highlights
local function createHighlight(character, color)
    if not character or character:FindFirstChildOfClass("Highlight") then return end
    local h = Instance.new("Highlight")
    h.Parent = character
    h.Adornee = character
    h.FillTransparency = 0.9
    h.OutlineColor = color or state.espColor
    h.OutlineTransparency = 0
    return h
end

local function clearHighlights()
    for _, plr in pairs(Players:GetPlayers()) do
        if plr.Character then
            for _, c in ipairs(plr.Character:GetChildren()) do
                if c:IsA("Highlight") then c:Destroy() end
            end
        end
    end
end

Tabs.Visuals:CreateToggle({
    Name="ESP Highlights",
    CurrentValue=state.esp,
    Callback=function(v)
        state.esp=v
        if v then
            for _,plr in pairs(Players:GetPlayers()) do
                if plr~=LocalPlayer and plr.Character then createHighlight(plr.Character,state.espColor) end
            end
            Players.PlayerAdded:Connect(function(p)
                p.CharacterAdded:Connect(function(c)
                    task.wait(0.3)
                    if state.esp then createHighlight(c,state.espColor) end
                end)
            end)
            notifyOk("ESP enabled")
        else
            clearHighlights()
            notifyNo("ESP disabled")
        end
    end
})

-- ESP Color Picker
Tabs.Visuals:CreateColorPicker({Name="ESP Color", Default=state.espColor, Callback=function(c)
    state.espColor=c
    if state.esp then clearHighlights(); for _,plr in pairs(Players:GetPlayers()) do if plr.Character and plr~=LocalPlayer then createHighlight(plr.Character,c) end end end
end})

