-- Music Player
local function playMusic(id, vol)
    vol = vol or state.musicVolume
    local s=Instance.new("Sound",workspace)
    s.SoundId="rbxassetid://"..tostring(id)
    s.Volume=vol
    s.Looped=true
    s:Play()
    return s
end

Tabs.Misc:CreateInput({Name="Play Music (Asset ID)", PlaceholderText="ID", RemoveTextAfterFocusLost=false, Callback=function(val)
    if tonumber(val) then
        local s = playMusic(tonumber(val))
        notifyOk("Music started")
        Tabs.Misc:CreateButton({Name="Stop Music", Callback=function() if s then s:Stop(); s:Destroy(); notifyOk("Music stopped") end end})
    else notifyNo("Invalid ID") end
end})

